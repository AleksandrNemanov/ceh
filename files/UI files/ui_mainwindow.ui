/* Стиль для вертикальной полосы прокрутки */
QScrollBar:vertical {
    border: none;
    background-color: #2E2E2E;  /* Цвет фона */
    width: 10px;                 /* Ширина полосы прокрутки */
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:vertical {
    background-color: #5E5E5E;  /* Цвет ползунка */
    min-height: 10px;
    border-radius: 0px;          /* Отключение закругления углов ползунка */
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    background-color: #4A4A4A;  /* Цвет кнопок вверх и вниз */
    height: 10px;               /* Размер кнопок */
    subcontrol-origin: margin;
    subcontrol-position: top;
    border-radius: 0px;          /* Отключение закругления углов */
}

QScrollBar::add-line:vertical:hover,
QScrollBar::sub-line:vertical:hover {
    background-color: #6E6E6E;  /* Цвет кнопок при наведении */
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    background-color: #2E2E2E;  /* Фон при пустом пространстве полосы прокрутки */
}

/* Стиль для горизонтальной полосы прокрутки */
QScrollBar:horizontal {
    border: none;
    background-color: #2E2E2E;  /* Цвет фона */
    height: 10px;                /* Высота горизонтальной полосы прокрутки */
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:horizontal {
    background-color: #5E5E5E;  /* Цвет ползунка */
    min-width: 10px;
    border-radius: 0px;          /* Отключение закругления углов */
}

QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
    background-color: #4A4A4A;  /* Цвет кнопок влево и вправо */
    width: 10px;                /* Размер кнопок */
    subcontrol-origin: margin;
    subcontrol-position: left;
    border-radius: 0px;          /* Отключение закругления углов */
}

QScrollBar::add-line:horizontal:hover,
